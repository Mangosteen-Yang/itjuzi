<script>
	function dateFormat(format, timestamp){
					    var a, jsdate=((timestamp) ? new Date(timestamp*1000) : new Date());
					    var pad = function(n, c){
					        if((n = n + "").length < c){
					            return new Array(++c - n.length).join("0") + n;
					        } else {
					            return n;
					        }
					    };
					    var txt_weekdays = ["Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"];
					    var txt_ordin = {1:"st", 2:"nd", 3:"rd", 21:"st", 22:"nd", 23:"rd", 31:"st"};
					    var txt_months = ["", "January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
					    var f = {
					        // Day
					        d: function(){return pad(f.j(), 2)},
					        D: function(){return f.l().substr(0,3)},
					        j: function(){return jsdate.getDate()},
					        l: function(){return txt_weekdays[f.w()]},
					        N: function(){return f.w() + 1},
					        S: function(){return txt_ordin[f.j()] ? txt_ordin[f.j()] : 'th'},
					        w: function(){return jsdate.getDay()},
					        z: function(){return (jsdate - new Date(jsdate.getFullYear() + "/1/1")) / 864e5 >> 0},

					        // Week
					        W: function(){
					            var a = f.z(), b = 364 + f.L() - a;
					            var nd2, nd = (new Date(jsdate.getFullYear() + "/1/1").getDay() || 7) - 1;
					            if(b <= 2 && ((jsdate.getDay() || 7) - 1) <= 2 - b){
					                return 1;
					            } else{
					                if(a <= 2 && nd >= 4 && a >= (6 - nd)){
					                    nd2 = new Date(jsdate.getFullYear() - 1 + "/12/31");
					                    return date("W", Math.round(nd2.getTime()/1000));
					                } else{
					                    return (1 + (nd <= 3 ? ((a + nd) / 7) : (a - (7 - nd)) / 7) >> 0);
					                }
					            }
					        },

					        // Month
					        F: function(){return txt_months[f.n()]},
					        m: function(){return pad(f.n(), 2)},
					        M: function(){return f.F().substr(0,3)},
					        n: function(){return jsdate.getMonth() + 1},
					        t: function(){
					            var n;
					            if( (n = jsdate.getMonth() + 1) == 2 ){
					                return 28 + f.L();
					            } else{
					                if( n & 1 && n < 8 || !(n & 1) && n > 7 ){
					                    return 31;
					                } else{
					                    return 30;
					                }
					            }
					        },

					        // Year
					        L: function(){var y = f.Y();return (!(y & 3) && (y % 1e2 || !(y % 4e2))) ? 1 : 0},
					        //o not supported yet
					        Y: function(){return jsdate.getFullYear()},
					        y: function(){return (jsdate.getFullYear() + "").slice(2)},
					        // Time
					        a: function(){return jsdate.getHours() > 11 ? "pm" : "am"},
					        A: function(){return f.a().toUpperCase()},
					        B: function(){
					            // peter paul koch:
					            var off = (jsdate.getTimezoneOffset() + 60)*60;
					            var theSeconds = (jsdate.getHours() * 3600) + (jsdate.getMinutes() * 60) + jsdate.getSeconds() + off;
					            var beat = Math.floor(theSeconds/86.4);
					            if (beat > 1000) beat -= 1000;
					            if (beat < 0) beat += 1000;
					            if ((String(beat)).length == 1) beat = "00"+beat;
					            if ((String(beat)).length == 2) beat = "0"+beat;
					            return beat;
					        },
					        g: function(){return jsdate.getHours() % 12 || 12},
					        G: function(){return jsdate.getHours()},
					        h: function(){return pad(f.g(), 2)},
					        H: function(){return pad(jsdate.getHours(), 2)},
					        i: function(){return pad(jsdate.getMinutes(), 2)},
					        s: function(){return pad(jsdate.getSeconds(), 2)},
					        //u not supported yet

					        // Timezone
					        //e not supported yet
					        //I not supported yet
					        O: function(){
					            var t = pad(Math.abs(jsdate.getTimezoneOffset()/60*100), 4);
					            if (jsdate.getTimezoneOffset() > 0) t = "-" + t; else t = "+" + t;
					            return t;
					        },
					        P: function(){var O = f.O();return (O.substr(0, 3) + ":" + O.substr(3, 2))},
					        //T not supported yet
					        //Z not supported yet

					        // Full Date/Time
					        c: function(){return f.Y() + "-" + f.m() + "-" + f.d() + "T" + f.h() + ":" + f.i() + ":" + f.s() + f.P()},
					        //r not supported yet
					        U: function(){return Math.round(jsdate.getTime()/1000)}
					    };

					    return format.replace(/[\\]?([a-zA-Z])/g, function(t, s){
					        if( t!=s ){
					            // escaped
					            ret = s;
					        } else if( f[s] ){
					            // a date function exists
					            ret = f[s]();
					        } else{
					            // nothing special
					            ret = s;
					        }
					        return ret;
					    });
					};
</script>
<script>
	//
	function ui_jkpopup_die(die_type){
		$('body').removeClass('no-scroll');

		var mask = $('body').find('.pop-mask');
		var popup = mask.next('div');
		var ui_popup = $('body').find('.ui-jkpopup');

		var die_type = die_type || 'fade-out';

		popup.addClass(die_type);
		mask.addClass('fade-out');

		setTimeout($.proxy(function(){
			ui_popup.remove();
		}),400);
	}
	//popup
	function ui_jkpopup_confirm(text,confirm_fuc){

		var template = '<div class="ui-jkpopup">\
			<div class="pop-mask"></div>\
			<div class="popup-confirm">\
				<div class="title"></div>\
				<div class="cont">\
					<p>'+ text +'<p>\
				</div>\
				<div class="btnset">\
					<p>\
						<a class="jzbtn c act-jkpopup-confirm">确定</a>\
						<a class="jzbtn bg-gray act-jkpopup-close">取消</a>\
					</p>\
				</div>\
			</div>\
		</div>';

		if(text === 'act-confirm'){
			confirm_fuc();
		}else if( text.indexOf('act-alert:') >= 0 ){
			template = template.replace('act-alert:','');
			template = template.replace('<a class="jzbtn bg-gray act-jkpopup-close">取消</a>','')
		}

		$('body').addClass('no-scroll');
		$('body').append(template);


		if(confirm_fuc){
			$('.popup-confirm').find('.act-jkpopup-confirm').on('click',function(){
				confirm_fuc();
			});
		}
		//关闭
		$('body').on('click','.act-jkpopup-confirm,.act-jkpopup-close',function(){
			ui_jkpopup_die();
			$('body').removeClass('no-scroll');
		});
	}

	//ugc--
	if($(".ugc-company-tip").size() > 0 ){
		$(".ugc-tip-toggle").click(function(){
			$(this).closest(".ugc-company-tip").toggleClass("open");
		});

		$('body').on('click','.act-ugc',function(){
			var thebtn = $(this);
			var ugcitem = $(this).closest('.ugc-block-item');

			//编辑按钮
			if( thebtn.hasClass('act-ugc-edit') ){

				if( thebtn.hasClass('disable') ){
					return false;
				};

				var data_editinfo = $('span[data-editinfo]:first').attr('data-editinfo');
				var data_edit_basenum = $('span[data-edit-basenum]:first').attr('data-edit-basenum');

				var editinfo = JSON.parse(data_editinfo);
				var edit_basenum = JSON.parse(data_edit_basenum);

				var sec_title = $(this).closest('.sec').find('.titlebar').text();

				var confirm_text = '';
				//基本信息1
				if( sec_title == '' ){
					if(editinfo.length > 0){
						for( var key in editinfo ){
							if( editinfo[key]['model_id'] == 1){
								var last_info = editinfo[key];
								confirm_text = '用户 ' + last_info['username'] + ' 已经在 ' + dateFormat('Y-m-d H:i:s',last_info['created_at']) + ' 更新过此模块信息，你编辑的信息将覆盖此前信息，确认编辑？';
							}
						}
					}
						
				};
				//基本信息2
				if(  sec_title.indexOf('基本信息') >= 0 ){
					
					if(editinfo.length > 0){
						for( var key in editinfo ){
							if( editinfo[key]['model_id'] == 2){
								var last_info = editinfo[key];
								confirm_text = '用户 ' + last_info['username'] + ' 已经在 ' + dateFormat('Y-m-d H:i:s',last_info['created_at']) + ' 更新过此模块信息，你编辑的信息将覆盖此前信息，确认编辑？';
							}
						}
					}
				}

				if( confirm_text === '' ){

					if( $('.on-edit').size() > 0 ){
						alert('页面还有正在编辑的项目，请保存或取消。');
					}else{
						ugcitem.addClass('on-edit');
					}
				}else{
					ui_jkpopup_confirm(confirm_text,function(){

						if( $('.on-edit').size() > 0 ){
							alert('页面还有正在编辑的项目，请保存或取消。');
						}else{
							ugcitem.addClass('on-edit');
						}
					});
				}
			};

			//取消按钮
			if( thebtn.hasClass('act-ugc-cancel') ){
				if( ugcitem.hasClass('on-newed') ){
					ugcitem.closest('ul').remove();
				}else{
					ugcitem.removeClass('on-edit');
					ugcitem.find('input[value]').each(function(){
						var val = $(this).attr('value');
						$(this).val(val);
					});
				}
			};

			//添加按钮
			if( thebtn.hasClass('act-ugc-add') ){

				if( $('.on-edit').size() > 0 ){
					ui_jkpopup_confirm('act-alert:页面还有正在编辑的项目，请保存或取消。');
				}else{

					var sec_title = $(this).closest('.sec').find('.titlebar').text();
					var confirm_text = '';

					var data_edit_othernum = $('span[data-edit-othernum]:first').attr('data-edit-othernum');
					var edit_othornum = JSON.parse(data_edit_othernum);


					//产品
					if( sec_title.indexOf('产品') >= 0 ){
						var num = 0;
						for( var keyi in edit_othornum ){
							if( edit_othornum[keyi]['model_id'] == 6){
								num = edit_othornum[keyi]['num'];
							}
						}

						if( num >= 10){
							confirm_text = 'act-alert:抱歉，你今天的添加数量已经达到上限（10条），休息一下明天再来吧。';
						}
					};
					//新闻
					if( sec_title.indexOf('新闻') >= 0 ){
						var num = 0;
						for( var keyi in edit_othornum ){
							if( edit_othornum[keyi]['model_id'] == 7){
								num = edit_othornum[keyi]['num'];
							}
						}

						if( num >= 10){
							confirm_text = 'act-alert:抱歉，你今天的添加数量已经达到上限（10条），休息一下明天再来吧。';
						}
					};
					//里程碑
					if( sec_title.indexOf('里程碑') >= 0 ){
						var num = 0;
						for( var keyi in edit_othornum ){
							if( edit_othornum[keyi]['model_id'] == 8){
								num = edit_othornum[keyi]['num'];
							}
						}
						if( num >= 10){
							confirm_text = 'act-alert:抱歉，你今天的添加数量已经达到上限（10条），休息一下明天再来吧。';
						}
					};

					var html_temp = $(this).closest('.sec').find('script[type="text/temp"]:first').html();
					var target = $(this).closest('.sec').find('ul:first');

					if( !confirm_text ){
						target.before(html_temp);
					}else{
						ui_jkpopup_confirm(confirm_text);
					}

				}
			};

			//save按钮
			if( thebtn.hasClass('act-ugc-save') ){
				var data_editinfo = $('span[data-editinfo]:first').attr('data-editinfo');
				var data_edit_basenum = $('span[data-edit-basenum]:first').attr('data-edit-basenum');

				
				var editinfo = JSON.parse(data_editinfo);
				var edit_basenum = JSON.parse(data_edit_basenum);

				var sec_title = $(this).closest('.sec').find('.titlebar').text();


				if( $(this).closest('.ugc-block-item').find('.on-uploading').size() > 0 ){

					alert('有正在上传的项目，请上传完成后提交');
					return false;
				}

				var confirm_text = '';
				//基本信息1
				if( sec_title == '' ){
					var num = 0;
					for( var keyi in edit_basenum ){
						if( edit_basenum[keyi]['model_id'] == 1){
							num = edit_basenum[keyi]['num'];
						}
					}
					if(num == 1){
						confirm_text = '这是你今天最后一次机会更新此模块信息了，\n仔细核对后确认提交！';
					}else if(num == 0){
						confirm_text = '每天只有两次编辑此模块的机会，\n仔细核对后确认提交！';
					}

					if( !confirm(confirm_text) ){
						return false;
					}
				};
				//基本信息2
				if( sec_title.indexOf('基本信息') >= 0 ){
					var num = 0;
					for( var keyi in edit_basenum ){
						if( edit_basenum[keyi]['model_id'] == 2){
							num = edit_basenum[keyi]['num'];
						}
					}
					if(num == 1){
						confirm_text = '这是你今天最后一次机会更新此模块信息了，请仔细核对！';
					}else if(num == 0){
						confirm_text = '每天只有两次编辑此模块的机会，请谨慎编辑！';
					}

					if( !confirm(confirm_text) ){
						return false;
					}
				}
			};
		});

		(function(){
			// check edit_num
			
			var data_editinfo = $('span[data-editinfo]:first').attr('data-editinfo');
			var data_edit_basenum = $('span[data-edit-basenum]:first').attr('data-edit-basenum');
			
			var editinfo = JSON.parse(data_editinfo);
			var edit_basenum = JSON.parse(data_edit_basenum);

			var num_base1 = 0;
			var num_base2 = 0;
			for( var keyi in edit_basenum ){
				if( edit_basenum[keyi]['model_id'] == 1){
					num_base1 = edit_basenum[keyi]['num'];
				}
				if( edit_basenum[keyi]['model_id'] == 2){
					num_base2 = edit_basenum[keyi]['num'];
				}
			}

			if( num_base1 >= 2){
				$('.act-ugc-edit-base1').addClass('disable');
			}
			if( num_base2 >= 2){
				$('.act-ugc-edit-base2').addClass('disable');
			}
		})();
	}

	//临时
	function ui_jkpopup_die(die_type){
		$('body').removeClass('no-scroll');

		var mask = $('body').find('.pop-mask');
		var popup = mask.next('div');
		var ui_popup = $('body').find('.ui-jkpopup');

		var die_type = die_type || 'fade-out';

		popup.addClass(die_type);
		mask.addClass('fade-out');

		setTimeout($.proxy(function(){
			ui_popup.remove();
		}),400);
	}

	$('body').on('click','.act-jkpopup-close',function(){
		ui_jkpopup_die('fly-out-top');
	});
</script>
<script>
(function($) {
	$.extend($.fn, {
		jknanoo: function(data, template) {
            //tools
            function isjson(obj) {
                return Object.prototype.toString.call(obj).toLowerCase() == "[object object]" && !obj.length;
            }

            function isarray(obj) {
                return toString.apply(obj).toLowerCase() === '[object array]';
            }

            function nanoo(template, data) {
                return template.replace(/\{\{([\w\.]*)\}\}/g, function(key, val) {

                    // console.log(typeof data[val]);

                    if (typeof data[val] == 'string' || typeof data[val] == 'number') {
                        // console.log('nanoo string');
                        return data[val];
                    } else if (typeof data[val] == 'object') {
                        // console.log('nanoo object');

                        var itemsethtml = '';
                        for (ikey in data[val]) {
                            // 处理array
                            // console.log(data[val][ikey]);

                            itemsethtml += '<a>' + (data[val][ikey]) + '</a>'; //指定用a包裹
                        }

                        return itemsethtml;
                    } else if (data[val] == undefined) {
                        // console.log('nanoo undefined');
                        return '';
                    } else {
                        return 'jkinfo: a value died here.';
                    }
                });
            }
            //
            var html = '';
            if (typeof(data) == 'string') {
                data = JSON.parse(data)
            };
            if (isjson(data)) {
                //处理json
                html = nanoo(template, data);
            } else if (isarray(data)) {
                //处理array
                for (var i in data) {
                    html += nanoo(template, data[i]) + '\n';
                }
            } else {
                //处理错误格式
                if (typeof data == 'string' && data.indexOf('<') == 0) {
                    html = 'jkinfo: it looks like an error of php outputs';
                } else {
                    html = "jkinfo: datatype should be only json or array~";
                }
            }
            //
            return html;
        },
		img_upload_jk: function(){
			var label = $(this);
			var input = label.find('input:first');
			var img = label.find('img:first');
			var tip = label.find('.tip:first');

			label.on('click',function(){

				if( $(this).hasClass('on-uploading') ){
					alert('图片上传中，请稍后');
					return false;
				}
			});

			input.on('change',function(e){

				label.addClass('on-uploading');

				// 检测是否支持File API
				if (window.File && window.FileReader && window.FileList && window.Blob) {
					//ok
				} else {
					alert('您的浏览器过旧，\nLogo/截图/头像等图片保存可能会失败。');
					return false;
				}
				
				//渲染图片
					var file =  e.target.files[0]; //获取图片资源
					
					// 检验格式
						if (!file.type.match('image.*')) {
							tip.html('请选择图片文件');
							img.attr('src','');
							return false;;
						};

						var reader = new FileReader();
						reader.readAsDataURL(file); // 读取文件
						// 渲染文件
						reader.onload = function(arg) {
							img.attr('src', arg.target.result);
						}

					//ajax 提交图片
						var form_data = new FormData();
						form_data.append(input.attr('name'),file);

						label.addClass('on-uploading');
						$.ajax({
							type: "POST", // 上传文件要用POST
							url: input.attr('posturl'),
							dataType : "json",
							crossDomain: true, // 如果用到跨域，需要后台开启CORS
							processData: false,  // 注意：不要 process data
							contentType: false,  // 注意：不设置 contentType
							data: form_data
						}).success(function(res) {
							if( typeof res == 'string'){
								res = JSON.parse(res);
							};
							label.removeClass('on-uploading');
							label.find('input[type="hidden"]').val(res['file_name']);
						}).fail(function(res) {
							label.removeClass('on-uploading');
							tip.html('上传失败 请重试');
							img.attr('src','');
						});
			});
			
		},
		imgs_upload_jk: function(){
			var block = $(this);
			var label = block.find('.upload_btn:first');
			var input = label.find('input:first');

			var html_temp = block.find('script[type="text/temp"]').html();
			var ul = block.find('ul:first');

			var max_size = parseInt( input.attr('max-size') );

			function check_imgsNone(ul){
				var img_size = ul.find('li').size();

				if(img_size === 0){
					ul.after('<input class="com_logo_marknone" type="hidden" name="com_logo[]">');
				}else{
					ul.find('.com_logo_marknone').remove();
				};
			};

			label.on('click',function(){
				
				var img_size = ul.find('li').size();

				if($(this).hasClass('disable')){
					return false;
				};
				if(img_size >= max_size){
					alert('最多上传数量为 '+max_size);
					return false;
				};
			});
			ul.on('click','.btn-x',function(){
				$(this).closest('li').remove();
				
				check_imgsNone(ul);
			});
			input.on('change',function(e){

				// 检测是否支持File API
				if (window.File && window.FileReader && window.FileList && window.Blob) {
					//ok
				} else {
					alert('您的浏览器过旧，\nLogo/截图/头像等图片保存可能会失败。');
					return false;
				}
				
					//渲染图片
					var file =  e.target.files[0]; //获取图片资源
					var file_mark = Date.parse(new Date());
					
					// 检验格式
					if (!file.type.match('image.*')) {
						alert('请选择图片文件');
						return false;;
					};

					var reader = new FileReader();
					reader.readAsDataURL(file); // 读取文件

					// 渲染文件
					reader.onload = function(arg) {
						var html_li = $(this).jknanoo({img_src: arg.target.result,mark: file_mark},html_temp);
						ul.sortable('destroy');
						ul.append(html_li);
						ul.sortable();
					}

				//ajax 提交图片
				var form_data = new FormData();
				form_data.append(input.attr('name'),file);
				form_data.append('file_mark',file_mark);
				label.addClass('on-uploading');
				$.ajax({
					type: "POST", // 上传文件要用POST
					url: input.attr('posturl'),
					dataType : "json",
					crossDomain: true, // 如果用到跨域，需要后台开启CORS
					processData: false,  // 注意：不要 process data
					contentType: false,  // 注意：不设置 contentType
					data: form_data
				}).success(function(res) {
					if( typeof res == 'string'){
						res = JSON.parse(res);
					};
					label.removeClass('on-uploading');
					block.find('input[mark='+file_mark+']').val(res['file_name']).closest('li').removeClass('on-uploading');
					check_imgsNone(ul);
				}).fail(function(res) {
					label.removeClass('on-uploading');
				});
			});
		}
	})
})(window.jQuery || window.Zepto);

$('#img_upload_jk').img_upload_jk();
$('#imgs_upload_jk').imgs_upload_jk();
$('#owner_imgs_upload').imgs_upload_jk();
//显示按钮
if ( $('.ugc-company-tip').size() > 0) {
	$('.act-ugc-edit,.act-ugc-add,.ugc-news-unclick').addClass('onshow');
};
</script>
<script>
	//log备份生成
	function jkjsonkeytrans(datajson,keyjson){
		//datajson：待处理json，keyjson：新旧key名称

		var json_text = JSON.stringify(datajson);
		for( keyi in keyjson ){
			var key_old = '"'+keyi+'":';
			var key_new = '"'+keyjson[keyi]+'":';

			json_text = json_text.replace( eval('/' + key_old + '/g'),key_new);
			json_text = json_text.replace(key_old,key_new);
		};

		var json_new = JSON.parse(json_text);
		return json_new;
	};

	$('.act-ugc-edit').on('click',function(){
		var form = $(this).closest('.ugc-block-item').find('.on-edit-show form:first');
		var json = {};

		form.find('[name]').each(function(){
			var key = $(this).attr('name');

			var val = '';
			var tagName = $(this)[0].tagName;

			if(tagName == 'SELECT'){
				val = $.trim( $(this).find('option:selected').text() );
			}else{
				if( $(this).attr('type') == 'hidden' ){
					val = $(this).attr('value');
				}else{
					val = $(this).val();
				}

			}

			if(  key.indexOf('[]') > 0  ){
				//数组name
				val = !json[key] ? (val) : (val+','+val);
				json[key] = val;
			}else{
				//普通name
				json[key] = val;
			}
		});

		if( json["com_born_year"] ){

			json["com_born"] = json["com_born_year"] + '.' + json["com_born_month"];
			delete json['com_born_year'];
			delete json['com_born_month'];
		}

		delete json['uploadfile'];
		delete json['model_one_log'];
		delete json['model_two_log'];
		delete json['com_id'];
		delete json['model_id'];

		json = jkjsonkeytrans(json,{"com_fund_status_id":"com_fund_status","com_status_id":"com_status_name","com_logo[]":"com_logo","com_first_scope_id":"com_scope_id","com_second_scope_id":"com_sub_scope"});
		form.find('.input_log').val('').val( JSON.stringify(json) );

	});
</script>       
			
