			<script>
    $(function(){
                $("#upload_image").uploadify({
            swf: 'http://itjuzi.com/assets/common/uploadify/uploadify.swf',
            uploader: 'http://itjuzi.com/uploadify/get_upload_file',
            'buttonText' : '上传图片',
            'fileTypeDesc' : '您所选的文件',
            'fileTypeExts' : '*.gif; *.jpg; *.png',
            multi: false,
            debug: false,
            formData : {userdata:'','token':'814f6000d27eb466c440d92507481ce82bcd1b65'},
            'onUploadError' : function(file, errorCode, errorMsg, errorString) {
                alert(file.name + ' could not be uploaded: ' + errorString);
            },
            'onUploadSuccess'   : function (file, data, response) {
                 var return_file = eval('(' + data + ')');  
                 if ( typeof return_file.file_name != 'undefined' )
                 {
                    $("#com_logo").html("<img style='height:70px;float:left;margin-right:10px;' src='http://itjuzi.com/images/"+ return_file.file_name+"'><input type='hidden' name='com_lice_archive' value='"+ return_file.file_name+"'>");
                 } else {
                    alert(return_file.error);
                 }                               
            }
        });

    });
